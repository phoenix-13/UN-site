.footer
  padding-bottom 15px

.footer .logo_container
  height 80px !important
  margin-left 30px
  margin-right 30px

.footer .logo_container img
  height 100%

.footer .made_by_part
  padding-top 10px
  padding-left 20%
  padding-right 20%
  font-size 11px

.footer .sposors_list
  height 80px
  background-color #e7e7e7
  margin-top 30px
  margin-bottom 30px

.footer .sposors_list i
  margin-left 10px
  margin-right 10px

.footer_carusel .item img
  max-height 80px
  padding 10px

.footer_carusel .item
  text-align center

.owl-theme .owl-controls .owl-buttons div
  position absolute
  color #777
  background transparent

.owl-theme .owl-controls .owl-buttons .owl-prev
  left -60px
  top 10px

.owl-theme .owl-controls .owl-buttons .owl-next
  right -60px
  top 10px

.footer_carusel .owl-pagination
  display none
