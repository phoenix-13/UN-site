.footer
  padding-bottom 15px

.footer .logo_container
  height 80px !important
  margin-left 30px
  margin-right 30px

.footer .logo_container img
  height 100%

.footer .made_by_part
  padding-top 10px
  margin 0 auto
  font-size 11px

.footer .sposors_list
  height 115px
  background-color #e7e7e7
  margin-bottom 30px
  border-top 1px solid #ecf0f1

.footer .sposors_list i
  margin-left 10px
  margin-right 10px
  margin-top: 35px;

.footer_carusel .item
  text-align center

.owl-theme .owl-controls .owl-buttons div
  position absolute
  color #777
  background transparent

.owl-theme .owl-controls .owl-buttons .owl-prev
  left -60px
  top 10px

.owl-theme .owl-controls .owl-buttons .owl-next
  right -60px
  top 10px

#owl2-partners
  width 900px

#owl2-partners .owl-dots
  display none

#owl2-partners .owl-item img
  padding-top 10px
  height 105px
  width auto
  margin 0 auto

.footer .footer_row
  width 1024px
  margin 0 auto

.footer .owl-nav
  display none
