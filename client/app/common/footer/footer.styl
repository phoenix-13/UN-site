
.footer .logo_container
  height 70px !important
  margin-left 30px
  margin-right 30px

.footer .logo_container img
  height 100%

.footer .made_by_part
  padding-top 10px
  margin 0 auto
  font-size 11px

.footer .sposors_list
  height 130px
  padding-top 20px

.footer .sposors_list i
  margin-left 10px
  margin-right 10px
  margin-top 35px
  color #aa1a24
  font-size 22px

.footer_carusel .item
  text-align center

.owl-theme .owl-controls .owl-buttons div
  position absolute
  color #777
  background transparent

.owl-theme .owl-controls .owl-buttons .owl-prev
  left -60px
  top 10px

.owl-theme .owl-controls .owl-buttons .owl-next
  right -60px
  top 10px

#owl2-partners
  width 924px

#owl2-partners .owl-dots
  display none

#owl2-partners .owl-item .innner_item
  margin 5px
  border 1px solid #f2f2f2
  border-radius 3px

#owl2-partners .owl-item img
  padding 5px
  height 80px
  width auto
  margin 0 auto

.footer_row_wrapp
  background #dfe3e6
  padding-top 5px
  padding-bottom 5px

.footer .footer_row
  width 1000px
  margin 0 auto

.footer .owl-nav
  display none

.next.btn,
.prev.btn
  padding 0

.next.btn:active,
.next.btn:focus,
.prev.btn:active,
.prev.btn:focus
  outline none
  box-shadow none
