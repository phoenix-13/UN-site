.login_container
  position fixed
  height 100%
  width 100%
  background #fff
  z-index 1000

.login_wrapper
  border-radius 5px
  margin-top 1%
  border 1px solid rgba(0,0,0,0.1)

.login_wrapper .md-button
  width 100%
  margin 0

.logo_wrapp
  height 100px
 & .logo
   height 100%
   & img
     height 100%

.login_inputs_wrapp
  font-family: "BPG Nino Mtavruli Book", sans-serif;
  padding 30px

  & .login_inputs
    background rgba(200,200,200,0.3)
    border-radius 5px

.text_bold
  font-weight bold !important
